	-moz-border-radius: 6px;
	border-radius: 6px;
}

#content h1 {
	font-family: 'Georgia', Times, serif;
	font-weight: bold;
	font-style: italic;
	color: #355495;
	border-bottom: 1px solid #355495;
}

#content p {
